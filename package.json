{
  "name": "babel-minify",
  "version": "1.0.0",
  "description": "A collection of babel minification plugins",
  "bin": {
    "babel-minify": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amasad/babel-minify.git"
  },
  "author": "amasad",
  "bugs": {
    "url": "https://github.com/amasad/babel-minify/issues"
  },
  "scripts": {
    "test": "eslint src/ && jest",
    "build": "babel --presets es2015 src/ --out-dir lib/"
  },
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "homepage": "https://github.com/amasad/babel-minify#readme",
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-traverse": "^6.3.26",
    "lodash": "^3.10.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathPattern": [
      "src"
    ],
    "preprocessorIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-jest": "^5.3.0",
    "babel-preset-es2015": "^6.3.13",
    "bytes": "^2.1.0",
    "chalk": "^1.1.0",
    "cli-table": "^0.3.1",
    "closure-compiler": "^0.2.12",
    "jest-cli": "^0.6.1",
    "uglify-js": "^2.6.2",
    "eslint": "^2.7.0"
  }
}
